{
  "name": "frontend-template",
  "private": true,
  "scripts": {
    "start": "snowpack dev"
  },
  "dependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@emotion/babel-plugin": "11.1.2",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@hookform/resolvers": "2.0.1",
    "@lingui/cli": "3.3.0",
    "@lingui/conf": "3.3.0",
    "@lingui/core": "3.3.0",
    "@lingui/macro": "3.3.0",
    "@lingui/react": "3.3.0",
    "@material-ui/core": "5.0.0-alpha.20",
    "@material-ui/icons": "5.0.0-alpha.20",
    "@snowpack/plugin-babel": "2.1.6",
    "@snowpack/plugin-dotenv": "2.0.5",
    "@snowpack/plugin-optimize": "0.2.13",
    "@snowpack/plugin-react-refresh": "2.4.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/snowpack-env": "2.3.3",
    "babel-plugin-macros": "3.0.1",
    "dotenv": "8.2.0",
    "history": "5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "7.1.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "snowpack": "3.0.11",
    "typescript": "4.1.3",
    "zod": "3.0.0-alpha.33"
  },
  "lingui": {
    "locales": [
      "en",
      "ky"
    ],
    "catalogs": [
      {
        "path": "locales/{locale}",
        "include": [
          "./**/*"
        ],
        "exclude": [
          "node_modules/**",
          "public/**",
          "build/**",
          ".build/**",
          "types/**"
        ]
      }
    ],
    "format": "po",
    "formatOptions": {
      "origins": false
    },
    "compileNamespace": "es"
  }
}
